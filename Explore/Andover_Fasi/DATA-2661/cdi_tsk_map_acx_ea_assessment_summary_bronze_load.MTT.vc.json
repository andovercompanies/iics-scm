      "customQuery" : "WITH current_batch AS (\n    SELECT \n        bls.batch_id,\n        bls.source_start_cutoff_datetime,\n        bls.source_end_cutoff_datetime\n    FROM $$operational_schema.batch_load_summary bls\n    WHERE bls.source_system_code = '$$sourcesystemcode'\n      AND bls.batch_status = 'Inprogress'\n      AND bls.batch_id = (\n          SELECT MAX(batch_id)\n          FROM $$operational_schema.batch_load_summary\n          WHERE source_system_code = '$$sourcesystemcode'\n            AND batch_status = 'Inprogress'\n      )\n)\nSELECT DISTINCT\n    TRIM(metal_aeas.source_name) AS source_name,\n    TRIM(metal_aeas.load_type) AS load_type,\n    CAST(TRIM(metal_aeas.system_id) AS INT) AS system_id,\n    TRIM(metal_aeas.parent_id) AS parent_id,\n    TRIM(metal_aeas.cmm_container) AS cmm_container,\n    TRIM(metal_aeas.id) AS id,\n    TRIM(metal_aeas.assessment_id) AS assessment_id,\n    TRIM(metal_aeas.solution_id) AS solution_id,\n    TRIM(metal_aeas.solution_name) AS solution_name,\n    TRIM(metal_aeas.business_function) AS business_function,\n    TRIM(metal_aeas.assessed_entity) AS assessed_entity,\n    TRIM(metal_aeas.category) AS category,\n    TRIM(metal_aeas.score) AS score,\n    TRIM(metal_aeas.score_percentile) AS score_percentile,\n    TRIM(metal_aeas.normalized_score) AS normalized_score,\n    TRIM(metal_aeas.recommended_action_description) AS recommended_action_description,\n    TRIM(metal_aeas.recommended_action_code) AS recommended_action_code,\n    TRIM(metal_aeas.conclusion_description) AS conclusion_description,\n    TRIM(metal_aeas.conclusion_code) AS conclusion_code,\n    TRIM(metal_aeas.score_date) AS score_date,\n    TRIM(metal_aeas.assesment_detail) AS assesment_detail,\n    TRIM(metal_aeas.error) AS error,\n    CAST(TRIM(metal_aeas.row_id) AS BIGINT) AS row_id,\n    cb.batch_id AS batch_id,\n    bronze_aeas.acx_ea_assessment_summary_row_id,\n    bronze_aeas.acx_ea_assessment_summary_row_md5checksum\nFROM $$raw_schema.acx_ea_assessment_summary metal_aeas\nLEFT JOIN $$staging_schema.acx_ea_assessment_summary bronze_aeas\n    ON COALESCE(TRIM(metal_aeas.id), '') = COALESCE(CAST(bronze_aeas.id AS VARCHAR), '')\n   AND CAST(TRIM(metal_aeas.system_id) AS INT) = CAST(bronze_aeas.system_id AS INT)\n   AND COALESCE(TRIM(metal_aeas.parent_id), '') = COALESCE(bronze_aeas.parent_id, '')\n   AND COALESCE(TRIM(metal_aeas.cmm_container), '') = COALESCE(bronze_aeas.cmm_container, '')\n   AND bronze_aeas.acx_ea_assessment_summary_row_effective_end_datetime = '9999-12-31 00:00:00.000'\n   AND bronze_aeas.acx_ea_assessment_summary_row_status = 'Active'\nCROSS JOIN current_batch cb\nORDER BY \n    metal_aeas.system_id,\n    metal_aeas.parent_id,\n    metal_aeas.cmm_container,\n    metal_aeas.id;",
